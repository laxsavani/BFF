<%-include('header')%>
<main id="main" class="main">
    <%if(messages.success){%>

        <div class="alert alert-success my-2">
          <%=messages.success%>
        </div>
        <%}%> 
    <%for(var i of datas){%>
        <div class="position-relative div">
            <a href="<%=i.img%>">
                <img src="<%=i.img%>" alt="" height="400px">
            </a>
        </div>
        <%}%>
  </main><!-- End #main -->
  <%-include('footer')%>